#include <stdio.h>

int main(){
    int x;
    int y;
    int z;
    scanf("%d %d %d", &x, &y, &z);
    if (x <= y && x <= z && y <= z){
        printf("%d %d %d", x, y, z);
    }
    if (x <= z && x <= y && z <= y){
        printf("%d %d %d", x, z, y);
    }
    if (y <= x && y <= z && x<= z){
        printf("%d %d %d", y, x, z);
    }
    if (y <= z && y <= x && z <= x){
        printf("%d %d %d", y, z, x);
    }
    if (z <= x && z <= y && x <= y){
        printf("%d %d %d", z, x, y);
    }
    if (z <= y && z <= x && y <= x){
        printf("%d %d %d", z, y, x);
    }
    return 0;
}
